<?xml version="1.0"?>
<parameters>

    <!-- Connection details -->
    <dbtype>DBMS</dbtype>
    <driver>DRIVER</driver>
    <DBUrl>DBURL</DBUrl>
    <username>USER</username>
    <password>PASSWORD</password>
    <isolation>TRANSACTION_SERIALIZABLE</isolation>
    
    <!-- The workload -->   
    <terminals>SCALEFACTOR</terminals>    
    <works>
        <work>
            <time>TIME</time>
            <rate>RATE</rate>
            <weights>WEIGHTS</weights>
        </work>
    </works>
    
    <!-- R/S Procedures declaration -->
    <transactiontypes>
        <transactiontype>
            <name>CPU1</name>
        </transactiontype>
        <transactiontype>
            <name>CPU2</name>
        </transactiontype>
        <transactiontype>
            <name>IO1</name>
        </transactiontype>
        <transactiontype>
            <name>IO2</name>
        </transactiontype>
        <transactiontype>
            <name>CONTENTION1</name>
        </transactiontype>
        <transactiontype>
            <name>CONTENTION2</name>
        </transactiontype>
    </transactiontypes>
</parameters>
